{
  "manifest_version": 2,
  "name": "Ecwid CSV Generator",
  "description": "Generator CSV Files for Ecwid",
  "version": "0.0.1",
  "author": "lukarek",
  "browser_action": {
    "default_title": "Generations",
    "default_icon": "icon1.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js", "main.js"]
    }
  ],
  "permissions": ["activeTab"]
}
